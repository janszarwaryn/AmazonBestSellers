{
  "Serilog": {
    "Using": ["Serilog.Sinks.Console", "Serilog.Sinks.File"],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "logs/log-.txt",
          "rollingInterval": "Day"
        }
      }
    ]
  },
  "ConnectionStrings": {
    "DefaultConnection": "Configuration loaded from .env file"
  },
  "Jwt": {
    "Secret": "Configuration loaded from .env file",
    "Issuer": "Configuration loaded from .env file",
    "Audience": "Configuration loaded from .env file",
    "ExpireMinutes": "Configuration loaded from .env file"
  },
  "RapidAPI": {
    "Key": "Configuration loaded from .env file",
    "Host": "Configuration loaded from .env file",
    "BaseUrl": "Configuration loaded from .env file"
  },
  "Cors": {
    "AllowedOrigins": ["Configuration loaded from .env file"]
  },
  "IpRateLimiting": {
    "EnableEndpointRateLimiting": true,
    "StackBlockedRequests": false,
    "RealIpHeader": "X-Real-IP",
    "ClientIdHeader": "X-ClientId",
    "HttpStatusCode": 429,
    "GeneralRules": [
      {
        "Endpoint": "*",
        "Period": "1m",
        "Limit": 100
      },
      {
        "Endpoint": "*:/api/auth/login",
        "Period": "15m",
        "Limit": 5
      },
      {
        "Endpoint": "*:/api/auth/register",
        "Period": "1h",
        "Limit": 3
      }
    ]
  },
  "IpRateLimitPolicies": {
    "IpRules": []
  },
  "AllowedHosts": "*"
}
